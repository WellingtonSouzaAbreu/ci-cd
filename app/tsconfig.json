{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": false,
		"baseUrl": "./",
		"paths": {
			"@assets/*": [
				"src/assets/*"
			],
			"@components/*": [
				"src/presentation/components/*"
			],
			"@contexts/*": [
				"./src/contexts/*"
			],
			"@routes/*": [
				"./src/routes/*"
			],
			"@services/*": [
				"./src/services/*"
			],
			"@screens/*": [
				"src/presentation/screens/*"
			],
			"@utils/*": [
				"src/presentation/utils/*"
			],
			"@common/*": [
				"src/presentation/common/*"
			],
			"@presentation/*": [
				"./src/presentation/*"
			],
			"@domain/*": [
				"./src/domain/*"
			],
		}
	}
}