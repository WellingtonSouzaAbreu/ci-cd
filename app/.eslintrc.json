{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:@typescript-eslint/recommended",
        "airbnb",
        "airbnb/hooks"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react",
        "react-native"
    ],
    "ignorePatterns": [
        "assets/**/**",
        "services/firebase/index.ts",
        "ignoredLogs.ts",
        "*.d.ts",
        "*.json",
        "*.svg",
        "*.sql"
    ],
    "rules": {
        "import/no-default-export": "warn",
        "import/prefer-default-export": "off",
        "no-multiple-empty-lines": "warn",
        "no-multi-spaces": "warn",
        "key-spacing": "warn",
        "no-tabs": "off",
        /*   "react/props-types": [
            "error",
            {
                "ignore": [
                    "navigation",
                    "navigation.navigate"
                ]
            }
        ], */
        "indent": [
            "warn",
            "tab",
            {
                "SwitchCase": 1
            }
        ],
        "react/jsx-indent": [
            "warn",
            "tab"
        ],
        "linebreak-style": [
            "warn",
            "windows"
        ],
        "semi": [
            "warn",
            "never"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "jsx-quotes": [
            "warn",
            "prefer-single"
        ],
        "no-unused-vars": [
            "warn",
            {
                "args": "none",
                "caughtErrors": "none"
            }
        ],
        "react/jsx-indent-props": [
            "warn",
            "tab"
        ],
        "react/jsx-curly-brace-presence": [
            "warn",
            {
                "props": "always",
                "children": "always"
            }
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".jsx",
                    ".tsx",
                    ".ts"
                ]
            }
        ]
    }
}